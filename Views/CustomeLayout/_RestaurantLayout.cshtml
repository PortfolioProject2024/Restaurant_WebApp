        <script>
            document.querySelectorAll('.category-button').forEach(function (button) {
                button.addEventListener('click', function () {
                    var categoryId = this.getAttribute('data-category-id');
                    fetch('/Menu/FilterByCategory?categoryId=' + categoryId)
                        .then(response => response.text())
                        .then(html => {
                            document.getElementById('foodItemsList').innerHTML = html;
                            hideEditDeleteButtons();
                        });
                });
            });

            function hideEditDeleteButtons() {
                var editButtons = document.querySelectorAll('.edit-button');
                var deleteButtons = document.querySelectorAll('.delete-button');

                editButtons.forEach(function (button) {
                    button.style.display = 'none';
                });

                deleteButtons.forEach(function (button) {
                    button.style.display = 'none';
                });
            }

            function showFoodItemModal(imageUrl, foodName, foodPrice, foodDescription) {
                var modal = document.getElementById('foodItemModal');
                var modalImg = document.getElementById('modalImg');
                var modalFoodName = document.getElementById('modalFoodName');
                var modalFoodPrice = document.getElementById('modalFoodPrice');
                var modalFoodDescription = document.getElementById('modalFoodDescription');

                modal.style.display = 'block';
                modalImg.src = '/Files/' + imageUrl;
                modalFoodName.textContent = foodName;
                modalFoodPrice.textContent = foodPrice;
                modalFoodDescription.innerHTML = foodDescription;

                return false;
            }

            function closeModal() {
                var modal = document.getElementById('foodItemModal');
                modal.style.display = 'none';
            }

            function addToCartViaAjax(form) {
                var formData = new FormData(form);
                fetch(form.action, {
                    method: form.method,
                    body: formData
                })
                    .then(response => response.json())
                    .then(data => {
                        if (data.success) {
                            updateCartIcon(data.cartItemCount);
                        } else {
                            alert('Failed to add item to cart: ' + data.message);
                        }
                    })
                    .catch(error => {
                        console.error('Error adding item to cart:', error);
                        alert('An error occurred while adding item to cart.');
                    });

                return false;
            }

            function updateCartIcon(cartItemCount) {
                var cartCountSpan = document.querySelector('#cartItemCount');
                if (cartCountSpan) {
                    cartCountSpan.textContent = cartItemCount;
                }
            }

            document.addEventListener('DOMContentLoaded', (event) => {
                fetch('/UserOrder/GetCartItemCount')
                    .then(response => response.json())
                    .then(data => {
                        if (data.success) {
                            updateCartIcon(data.cartItemCount);
                        }
                    })
                    .catch(error => {
                        console.error('Error fetching cart item count:', error);
                    });
            });
        </script>




